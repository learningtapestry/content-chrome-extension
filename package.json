{
  "name": "content-chrome-extension",
  "scripts": {
    "prebuild-spa": "mkdir -p dist/spa ; cp -R src/spa/index.html dist/spa/",
    "build-spa": "webpack --progress --colors --inline --content-base dist/spa --config webpack.spa.js",
    "dev-spa": "npm run prebuild-spa ; webpack-dev-server --devtool eval --progress --colors --inline --content-base dist/spa --config webpack.spa.js"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11"
  },
  "dependencies": {
    "alt": "^0.18.1",
    "alt-utils": "^1.0.0",
    "axios": "^0.9.0",
    "eventemitter3": "^1.1.1",
    "font-awesome": "^4.5.0",
    "intersect": "^1.0.1",
    "normalize.css": "^3.0.3",
    "perfect-scrollbar": "^0.6.8",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "superagent": "^1.7.0",
    "transmitter": "^1.1.0",
    "unordered-array-remove": "^1.0.1",
    "webpack-dev-server": "^1.14.1"
  }
}
